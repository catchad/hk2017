<!DOCTYPE html>
html(lang="zh-tw")
	head
		meta(charset="UTF-8")
		meta(name="viewport", content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0")
		title video test
		link(rel="stylesheet", href="assets/styles/video.css")
	body
		div(class="video")
			div(class="video__screen")
				video(class="video__videoElement", id="myVideo", muted, playsinline)
					//- source(src="assets/videos/mv.mp4", type="video/mp4")
				audio(class="video__audioElement")
					//- source(src="assets/videos/testaudio.mp3", type="audio/mpeg")
				canvas(class="video__overlay", id="screenCanvas", width="1920", height="1080")
				canvas(class="video__overlay", id="fwCanvas")
			div(class="video__controls")
				//- div(class="video__play")
				img(class="video__play", src="assets/images/play.svg")
				div(class="video__timeline")
					div(class="video__progress")
				//- div(class="video__repeat")
		div(class="msg")
		script.
			var keyFrameData = [];
